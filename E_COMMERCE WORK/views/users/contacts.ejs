    }

    @media (max-width: 768px) {
        .contact-page h1 {
            font-size: 2.5rem;
        }

        .contact-page p.lead {
            font-size: 1.2rem;
        }

        .contact-page p {
            font-size: 1rem;
        }

        .icon-block {
            flex-direction: column;
            gap: 20px;
        }

        .icon-item i {
            font-size: 2rem;
        }

        .icon-item p {
            font-size: 0.9rem;
        }
    }
